{
    "require": {
        "doctrine/dbal": "^3.6",
        "doctrine/migrations": "^3.5",
        "doctrine/orm": "^2.14",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^9.51",
        "symfony/cache": "^6.2",
        "symfony/console": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^6.2"
    },

    "autoload": {
        "classmap": [
            "App/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
